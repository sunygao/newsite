#home
	include partials/_grid

	include partials/_main_nav

	section.images
		each project in data.projects
			if project.previewImages
				ul(class="image-wrapper", id=project.slug)
					each previewImage in project.previewImages
						li(style="background-image: url(../images/projects/" + project.slug + "/" + previewImage + ")")
					

	section.work
		ul
			each project in data.projects
				li
					a(href="/work/" + project.slug)
						h2= project.title
						h3= project.client
